{
  "name": "contain-by-screen",
  "version": "1.2.1",
  "description": "Position a dropdown element near a button in a way that fits on the screen.",
  "main": "js/index.js",
  "scripts": {
    "prepare": "rimraf js && babel -s inline -d js/ src/ && flow-copy-source -v src js",
    "test": "yarn run lint && yarn run flow_check && mocha",
    "flow_check": "flow check",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgentME/contain-by-screen.git"
  },
  "keywords": [
    "dropdown",
    "positioning",
    "menu"
  ],
  "author": "Chris Cowan <agentme49@gmail.com>",
  "license": "MIT",
  "browserify": {
    "transform": [
      "envify"
    ]
  },
  "bugs": {
    "url": "https://github.com/AgentME/contain-by-screen/issues"
  },
  "homepage": "https://github.com/AgentME/contain-by-screen#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-add-module-exports": "^0.3.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "eslint": "^5.0.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "flow-bin": "^0.78.0",
    "flow-copy-source": "^2.0.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "envify": "^4.1.0",
    "lodash": "^4.6.1"
  }
}
